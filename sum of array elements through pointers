
#include <iostream>

using namespace std;

int Sum_Arr(int *arr,int size){
    int sum=0,a=0;
    while(a<size){
        sum=sum+ *(arr+a);
        a++;
    }
    return sum;
}

int main()
{
    int size,i=0;
    cout<<"enter size"<<endl;
    cin>>size;
    
    int arr[size];
    cout<<"enter array elements"<<endl;
    
    while(i<size){
        cin>>arr[i];
        i++;
    }
    
   cout<< Sum_Arr(arr,size);

    return 0;
}
